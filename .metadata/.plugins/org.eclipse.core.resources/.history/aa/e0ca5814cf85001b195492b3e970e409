package variaveis;

public class TipoString {

	public static void main(String[] args) {
		
	String	email = "reginatosouza@gmal.com";
	System.out.println("Original: " + email);
	System.out.println("Minuscula: " + email.toLowerCase());
	System.out.println("Maiuscula: " + email.toUpperCase());
	System.out.println("Qtde Caracteres: " + email.length());
	System.out.println ("É igual: " + email.contentEquals("h@gama.com"));	
	System.out.println ("É igual 2 :" + email.equalsIgnoreCase("h@gama.com"));	
		
		
		
		
		
		
		
		
		
	}                                
	
	
	
	
}
